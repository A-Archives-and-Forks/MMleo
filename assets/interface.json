{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/../agent/main.py",
            "-u"
        ]
    },
    "task": [
        {
            "name": "启动",
            "entry": "StartUp"
        },
        {
            "name": "领取月卡钻石",
            "entry": "Entry_Store",
            "pipeline_override": {
                "CallOutMenu": {
                    "next": "StoreIcon"
                }
            }
        },
        {
            "name": "每日海选",
            "entry": "Entry_Recruit",
            "pipeline_override": {
                "CallOutMenu": {
                    "next": "RecruitIcon"
                }
            },
            "option": [
                "海选招募次数"
            ]
        },
        {
            "name": "检测打歌点位",
            "entry": "MusicTargetAreaSearch"
        },
        {
            "name": "星光演唱会",
            "entry": "Entry_StarConcert",
            "pipeline_override": {
                "for_end_after_concert_1": {
                    "next": "Flag_星光演唱会"
                },
                "Flag_星光演唱会": {
                    "next": "奖励领取_星光ver"
                },
                "彩歌需要数据下载_好的": {
                    "next": "星光演唱会打歌中"
                }
            }
        },
        {
            "name": "临时打歌",
            "entry": "星光演唱会打歌中",
            "pipeline_override": {
                "星光演唱会打歌中": {
                    "next": []
                }
            }
        },
        {
            "name": "合奏演唱会",
            "entry": "协同演唱会期间icon",
            "pipeline_override": {
                "确认歌曲": {
                    "next": "换上原唱"
                }
            }
        },
        {
            "name": "自动阅读",
            "entry": "StartReading"
        },
        {
            "name": "生日纪念活动",
            "entry": "Entry_BirthdayActivity",
            "pipeline_override": {
                "Flag_Main": {
                    "next": "BA_fromBanner",
                    "interrupt": "Swipe_forBA"
                }
            }
        },
        {
            "name": "合成游戏",
            "entry": "Entry_CompositeGame",
            "option": [
                "不使用药水资源箱"
            ]
        },
        {
            "name": "9周年通行证_测试用",
            "entry": "9Activity",
            "pipeline_override": {
                "9IconInMain": {
                    "template": "temp_activity/九周年系列/九周年火组活动主界面icon.png"
                }
            }
        }
    ],
    "option": {
        "不使用药水资源箱": {
            "cases": [
                {
                    "name": "yes",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action_param": {
                                "zyx4": 0
                            }
                        }
                    }
                },
                {
                    "name": "no",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action_param": {
                                "zyx4": 1
                            }
                        }
                    }
                }
            ]
        },
        "海选招募次数": {
            "cases": [
                {
                    "name": "招募一次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout1InAudition"
                        }
                    }
                },
                {
                    "name": "招募十次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout10InAudition"
                        }
                    }
                },
                {
                    "name": "招募百次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout100InAudition"
                        }
                    }
                }
            ]
        }
    }
}