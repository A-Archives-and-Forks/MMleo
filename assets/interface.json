{
    "name": "MMleo",
    "url": "https://github.com/fictionalflaw/MMleo",
    "mirrorchyan_rid": "MMleo",
    "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        },
        {
            "name": "B服",
            "path": [
                "{PROJECT_DIR}/resource/base",
                "{PROJECT_DIR}/resource/bside"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/../agent/main.py",
            "-u"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "Entry_StartUP"
        },
        {
            "name": "领取月卡钻石",
            "entry": "Entry_Store",
            "pipeline_override": {
                "CallOutMenu": {
                    "next": "StoreIcon"
                }
            }
        },
        {
            "name": "每日海选",
            "doc": "支持选择海选次数",
            "entry": "Entry_Recruit",
            "pipeline_override": {
                "CallOutMenu": {
                    "next": "RecruitIcon"
                }
            },
            "option": [
                "海选招募次数"
            ]
        },
        {
            "name": "每日练卡",
            "entry": "Entry_Card",
            "doc": "用于完成每日练卡任务，不会对卡片检索进行变更",
            "option": [
                "按照等级排序",
                "每日等级提升",
                "每日偶像之路"
            ]
        },
        {
            "name": "每日阅读",
            "entry": "Entry_Card",
            "doc": "用于完成每日阅读任务",
            "pipeline_override": {
                "ClickMenuIntale": {
                    "next": "SkipInTale",
                    "interrupt": []
                },
                "SureSkip": {
                    "next": [
                        "ClickScreenForSkip",
                        "StopRead",
                        "Flag_EndReading"
                    ]
                },
                "ClickScreenForSkip": {
                    "next": [
                        "ClickScreenForSkip",
                        "StopRead",
                        "Flag_EndReading"
                    ]
                }
            }
        },
        {
            "name": "协会日常",
            "entry": "Entry_GuildNormal",
            "doc": "只会进行签到和偶像互动，推荐放在任务列表中前置任务活跃度足够80的位置"
        },
        {
            "name": "事务所",
            "entry": "Entry_WorkingHouce",
            "doc": "偶像工作，点击齿轮选择细节。当前版本偶像工作不具备规律性，因此只能选择一定价值区间内的工作",
            "option": [
                "拜访事务所",
                "选择工作类型",
                "使用辅助工作",
                "使用推荐编队"
            ]
        },
        {
            "name": "检测打歌点位",
            "entry": "MusicTargetAreaSearch",
            "doc": "不更换界面布局的情况下只需要成功执行一次，更换后重新识别"
        },
        {
            "name": "星光演唱会_旧版",
            "entry": "Entry_StarConcert",
            "doc": "自动导航到星光演唱会，需要已检测过打歌点位",
            "pipeline_override": {
                "for_end_after_concert_1": {
                    "next": "Flag_星光演唱会"
                },
                "Flag_星光演唱会": {
                    "next": "奖励领取_星光ver"
                },
                "彩歌需要数据下载_好的": {
                    "next": "星光演唱会打歌中"
                }
            }
        },
        {
            "name": "星光演唱会",
            "entry": "Entry_StarConcert",
            "doc": "当前版本不会处理BP不足的情况，请酌情使用",
            "pipeline_override": {
                "for_end_after_concert_1": {
                    "next": "Flag_星光演唱会"
                },
                "Flag_星光演唱会": {
                    "next": "奖励领取_星光ver"
                },
                "彩歌需要数据下载_好的": {
                    "next": "星光演唱会打歌中"
                },
                "奖励领取_星光ver": {
                    "next": "for_search_music_in_star",
                    "interrupt": []
                },
                "确认歌曲_星光ver": {
                    "next": [
                        "Feat_难度调整"
                    ]
                },
                "Flag_目标难度": {
                    "next": [
                        "Feat_BP调整"
                    ]
                },
                "Flag_开始演唱会_next": {
                    "next": [
                        "StartTheConcert_星光ver"
                    ]
                },
                "Flag_已在目标BP": {
                    "next": [
                        "StartTheConcert_星光ver"
                    ]
                }
            },
            "option": [
                "调整消耗的BP数"
            ]
        },
        {
            "name": "领取日常奖励",
            "entry": "Entry_NormalAward",
            "doc": "用于日常奖励领取，不会识别日常进度"
        },
        {
            "name": "临时打歌",
            "doc": "用于各种需要临时打歌的情况，需要处于打歌界面",
            "entry": "星光演唱会打歌中",
            "pipeline_override": {
                "星光演唱会打歌中": {
                    "next": []
                }
            }
        },
        {
            "name": "合奏演唱会",
            "entry": "协同演唱会期间icon",
            "doc": "等待重构",
            "pipeline_override": {
                "确认歌曲": {
                    "doc": "确认歌曲被BP调整使用了，不应有next",
                    "next": "换上原唱"
                }
            }
        },
        {
            "name": "自动阅读",
            "entry": "StartReading"
        },
        {
            "name": "生日纪念活动",
            "entry": "Entry_BirthdayActivity",
            "doc": "自动导航到纪念活动界面并上交礼物，不提供礼物购买功能",
            "pipeline_override": {
                "Flag_Main": {
                    "next": "BA_fromBanner",
                    "interrupt": "Swipe_forBA"
                }
            }
        },
        {
            "name": "9周年通行证_测试用",
            "entry": "9Activity",
            "pipeline_override": {
                "9IconInMain": {
                    "template": "temp_activity/九周年系列/九周年火组活动主界面icon.png"
                }
            }
        }
    ],
    "option": {
        "海选招募次数": {
            "cases": [
                {
                    "name": "招募一次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout1InAudition"
                        }
                    }
                },
                {
                    "name": "招募十次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout10InAudition"
                        }
                    }
                },
                {
                    "name": "招募百次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout100InAudition"
                        }
                    }
                }
            ]
        },
        "调整消耗的BP数": {
            "cases": [
                {
                    "name": "0BP",
                    "pipeline_override": {}
                },
                {
                    "name": "1BP",
                    "pipeline_override": {
                        "Flag_已在目标BP": {
                            "expected": [
                                "BP -1"
                            ]
                        },
                        "BP调整_TO": {
                            "expected": [
                                "消耗1BP"
                            ]
                        }
                    }
                },
                {
                    "name": "2BP",
                    "pipeline_override": {
                        "Flag_已在目标BP": {
                            "expected": [
                                "BP -2"
                            ]
                        },
                        "BP调整_TO": {
                            "expected": [
                                "消耗2BP"
                            ]
                        }
                    }
                },
                {
                    "name": "3BP",
                    "pipeline_override": {
                        "Flag_已在目标BP": {
                            "expected": [
                                "BP -3"
                            ]
                        },
                        "BP调整_TO": {
                            "expected": [
                                "消耗3BP"
                            ]
                        }
                    }
                }
            ]
        },
        "按照等级排序": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "Flag_Card_next": {
                            "next": [
                                "组合选择_not等级",
                                "组合选择_in等级"
                            ]
                        },
                        "排序方式_in升序": {
                            "next": [
                                "Click_左上卡"
                            ]
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "每日等级提升": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "Click_左上卡": {
                            "next": [
                                "Card_等级提升"
                            ]
                        },
                        "Card_等级提升_关闭": {
                            "next": [
                                "Flag_卡片详情"
                            ]
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "每日偶像之路": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "Flag_卡片详情": {
                            "next": [
                                "Card_偶像之路"
                            ]
                        },
                        "Card_偶像之路_退出": {
                            "next": []
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "选择工作类型": {
            "cases": [
                {
                    "name": "期间限定_默认第一个",
                    "pipeline_override": {
                        "Flag_选择偶像工作_next": {
                            "next": [
                                "选择偶像工作_期间限定"
                            ]
                        }
                    }
                },
                {
                    "name": "每日工作_最后一个",
                    "pipeline_override": {
                        "选择偶像工作_每日工作": {
                            "next": "Swipe_最后_1"
                        }
                    }
                },
                {
                    "name": "每日工作_第一个",
                    "pipeline_override": {}
                }
            ]
        },
        "使用辅助工作": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "选择偶像工作_前往工作准备": {
                            "next": "Click_辅助工作"
                        },
                        "Click_辅助工作": {
                            "next": "Flag_参加工作成员_next"
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "使用推荐编队": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "Flag_参加工作成员_next": {
                            "next": "Click_推荐编队"
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "拜访事务所": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "WorkingHouceIcon": {
                            "next": "Start_拜访事务所"
                        },
                        "拜访_Count5次": {
                            "next": "Flag_WorkingHouce_next"
                        },
                        "CountTask_RunNode": {
                            "next": "拜访_红心已点",
                            "interrupt": "拜访_点击红心"
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        }
    }
}