{
    "name": "MMleo",
    "url": "https://github.com/fictionalflaw/MMleo",
    "mirrorchyan_rid": "MMleo",
    "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/../agent/main.py",
            "-u"
        ]
    },
    "task": [
        {
            "name": "启动",
            "entry": "Entry_StartUP"
        },
        {
            "name": "领取月卡钻石",
            "entry": "Entry_Store",
            "pipeline_override": {
                "CallOutMenu": {
                    "next": "StoreIcon"
                }
            }
        },
        {
            "name": "每日海选",
            "doc": "支持选择海选次数",
            "entry": "Entry_Recruit",
            "pipeline_override": {
                "CallOutMenu": {
                    "next": "RecruitIcon"
                }
            },
            "option": [
                "海选招募次数"
            ]
        },
        {
            "name": "协会日常",
            "entry": "Entry_GuildNormal",
            "doc": "只会进行签到和偶像互动，推荐放在任务列表中前置任务活跃度足够80的位置"
        },
        {
            "name": "检测打歌点位",
            "entry": "MusicTargetAreaSearch",
            "doc": "不更换界面布局的情况下只需要成功执行一次，更换后重新识别"
        },
        {
            "name": "星光演唱会_旧版",
            "entry": "Entry_StarConcert",
            "doc": "自动导航到星光演唱会，需要已检测过打歌点位",
            "pipeline_override": {
                "for_end_after_concert_1": {
                    "next": "Flag_星光演唱会"
                },
                "Flag_星光演唱会": {
                    "next": "奖励领取_星光ver"
                },
                "彩歌需要数据下载_好的": {
                    "next": "星光演唱会打歌中"
                }
            }
        },
        {
            "name": "星光演唱会",
            "entry": "Entry_StarConcert",
            "doc": "用于测试",
            "pipeline_override": {
                "for_end_after_concert_1": {
                    "next": "Flag_星光演唱会"
                },
                "Flag_星光演唱会": {
                    "next": "奖励领取_星光ver"
                },
                "彩歌需要数据下载_好的": {
                    "next": "星光演唱会打歌中"
                },
                "奖励领取_星光ver": {
                    "next": "for_search_music_in_star",
                    "interrupt": []
                }
            }
        },
        {
            "name": "领取日常奖励",
            "entry": "Entry_NormalAward",
            "doc": "用于日常奖励领取，不会识别日常进度"
        },
        {
            "name": "临时打歌",
            "doc": "用于各种需要临时打歌的情况，需要处于打歌界面",
            "entry": "星光演唱会打歌中",
            "pipeline_override": {
                "星光演唱会打歌中": {
                    "next": []
                }
            }
        },
        {
            "name": "合奏演唱会",
            "entry": "协同演唱会期间icon",
            "pipeline_override": {
                "确认歌曲": {
                    "next": "换上原唱"
                }
            }
        },
        {
            "name": "自动阅读",
            "entry": "StartReading"
        },
        {
            "name": "生日纪念活动",
            "entry": "Entry_BirthdayActivity",
            "doc": "自动导航到纪念活动界面并上交礼物，不提供礼物购买功能",
            "pipeline_override": {
                "Flag_Main": {
                    "next": "BA_fromBanner",
                    "interrupt": "Swipe_forBA"
                }
            }
        },
        {
            "name": "合成游戏",
            "entry": "Entry_CompositeGame",
            "doc": "每年只活一个月",
            "option": [
                "订单与资源箱管理"
            ]
        },
        {
            "name": "合成游戏_AC",
            "entry": "Entry_CompositeGame",
            "doc": "舍弃了部分稳定性的重构版，来体验加速的世界叭",
            "pipeline_override": {
                "Entry_CompositeGame": {
                    "custom_action": "CompositeGamePlayer_mini"
                }
            },
            "option": [
                "订单与资源箱管理_AC"
            ]
        },
        {
            "name": "9周年通行证_测试用",
            "entry": "9Activity",
            "pipeline_override": {
                "9IconInMain": {
                    "template": "temp_activity/九周年系列/九周年火组活动主界面icon.png"
                }
            }
        }
    ],
    "option": {
        "订单与资源箱管理": {
            "cases": [
                {
                    "name": "自动提交订单and使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action_param": {
                                "submit_order": 1,
                                "zyx4": 1
                            }
                        }
                    }
                },
                {
                    "name": "自动提交订单and不使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action_param": {
                                "submit_order": 1,
                                "zyx4": 0
                            }
                        }
                    }
                },
                {
                    "name": "不提交订单and使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action_param": {
                                "submit_order": 0,
                                "zyx4": 1
                            }
                        }
                    }
                },
                {
                    "name": "不提交订单and不使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action_param": {
                                "submit_order": 0,
                                "zyx4": 0
                            }
                        }
                    }
                }
            ]
        },
        "订单与资源箱管理_AC": {
            "cases": [
                {
                    "name": "自动提交订单and使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action": "CompositeGamePlayer_mini",
                            "custom_action_param": {
                                "submit_order": 1,
                                "zyx4": 1
                            }
                        }
                    }
                },
                {
                    "name": "自动提交订单and不使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action": "CompositeGamePlayer_mini",
                            "custom_action_param": {
                                "submit_order": 1,
                                "zyx4": 0
                            }
                        }
                    }
                },
                {
                    "name": "不提交订单and使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action": "CompositeGamePlayer_mini",
                            "custom_action_param": {
                                "submit_order": 0,
                                "zyx4": 1
                            }
                        }
                    }
                },
                {
                    "name": "不提交订单and不使用药水资源箱",
                    "pipeline_override": {
                        "Entry_CompositeGame": {
                            "custom_action": "CompositeGamePlayer_mini",
                            "custom_action_param": {
                                "submit_order": 0,
                                "zyx4": 0
                            }
                        }
                    }
                }
            ]
        },
        "海选招募次数": {
            "cases": [
                {
                    "name": "招募一次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout1InAudition"
                        }
                    }
                },
                {
                    "name": "招募十次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout10InAudition"
                        }
                    }
                },
                {
                    "name": "招募百次",
                    "pipeline_override": {
                        "AuditionInRecruit": {
                            "next": "Scout100InAudition"
                        }
                    }
                }
            ]
        }
    }
}